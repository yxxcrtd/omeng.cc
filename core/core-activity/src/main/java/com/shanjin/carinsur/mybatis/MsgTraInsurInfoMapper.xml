		where t.order_no in 
		<foreach collection="list" item="ids" open="(" separator="," close=")">
			#{ids}
		</foreach>		
	</delete>
	
	<select id="getEntityByKey" resultMap="MsgTraInsurInfoBsRstMap" parameterType="java.lang.String">
