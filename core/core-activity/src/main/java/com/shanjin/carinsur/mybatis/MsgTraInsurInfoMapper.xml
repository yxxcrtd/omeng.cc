	<!-- 通用的sql片段  -->
	<sql id="commSelTpl">
		SELECT 
			t.order_no as orderNo, 
			t.sun_order_no as sunOrderNo, 
			t.proposal_no as proposalNo, 
			t.policy_no as policyNo, 
			t.start_date as startDate, 
			t.end_date as endDate, 
			t.premium as premium, 
			t.tax as tax, 
			t.pay_time as payTime, 
			t.out_pay_id as outPayId, 
			t.callback_detail as callbackDetail, 
			t.callback_pay as callbackPay, 
			t.pc_pay_url as pcPayUrl
		FROM msg_tra_insur_info AS t
	</sql>
	
	<sql id="commSelCount">
		SELECT COUNT(1) FROM msg_tra_insur_info AS t
	</sql>
	
	<sql id="commInsTpl">
		INSERT INTO msg_tra_insur_info (
				order_no,
				sun_order_no, 
				proposal_no, 
				policy_no, 
				start_date, 
				end_date, 
				premium, 
				tax, 
				pay_time, 
				out_pay_id, 
				callback_detail, 
				callback_pay, 
				pc_pay_url
			) VALUES (
				#{orderNo},
				#{sunOrderNo}, 
				#{proposalNo}, 
				#{policyNo}, 
				#{startDate}, 
				#{endDate}, 
				#{premium}, 
				#{tax}, 
				#{payTime}, 
				#{outPayId}, 
				#{callbackDetail}, 
				#{callbackPay}, 
				#{pcPayUrl}
			)
	</sql>
	
	<sql id="commUpdTpl">
		UPDATE msg_tra_insur_info AS t 
	</sql>
	
	<sql id="commDelTpl">
		DELETE t FROM msg_tra_insur_info AS t
	</sql>
	
	<sql id="commOrderByTpl">
		ORDER BY order_no DESC
	</sql>
	
	
	<!-- +++++++++++++++++++++++++++++++++++公共方法Start+++++++++++++++++++++++++++++++++++ -->
	<insert id="saveEntity" parameterType="com.shanjin.carinsur.model.MsgTraInsurInfo">

		<include refid="commInsTpl"/>
	</insert>
	
	<update id="updateEntity" parameterType="com.shanjin.carinsur.model.MsgTraInsurInfo">
		<include refid="commUpdTpl"/>
		<trim prefix="SET" suffixOverrides=",">
			<if test="null!=sunOrderNo and sunOrderNo!=''">
				t.sun_order_no = #{sunOrderNo},
			</if>		
			<if test="null!=proposalNo and proposalNo!=''">
				t.proposal_no = #{proposalNo},
			</if>		
			<if test="null!=policyNo and policyNo!=''">
				t.policy_no = #{policyNo},
			</if>		
			<if test="null!=startDate and startDate!=''">
